        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <include file="classpath:/db/changelog/changes/liq-activiti.xml"/>
    <include file="changes/liq_Sequences.xml" relativeToChangelogFile="true"/>
    <include file="changes/liq_Entity.xml" relativeToChangelogFile="true"/>
    <include file="changes/liq_Changes.xml" relativeToChangelogFile="true"/>
    <include file="changes/liq_UniqueConstraint.xml" relativeToChangelogFile="true"/>
    <include file="changes/liq_ForeignKeyConstraint.xml" relativeToChangelogFile="true"/>
    <include file="changes/liq_View.xml" relativeToChangelogFile="true"/>
    <include file="changes/liq_Index.xml" relativeToChangelogFile="true"/>

</databaseChangeLog>
